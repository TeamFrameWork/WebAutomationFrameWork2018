<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Amazon Automation Script " parallel="false">

    <parameter name="useCloudEnv" value="true"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="Windows"/>
    <parameter name="os_version" value="10"/>
    <parameter name="browserName" value="Chrome"/>
    <parameter name="browserVersion" value="60.0"/>
    <parameter name="url" value="https://www.nbc.com"/>

    <test name = "NBC homeTest" enabled="true">
        <classes>
            <!--<class name="homeTest.TestHomePage"/>-->
           <class name="homeTest.TestShowsPage"/>
        </classes>
    </test>

</suite>
